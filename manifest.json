{
  "manifest_version": 2,
  "name": "Goldbug590",
  "version": "1.0.0",
  "description": "Nothing suspicious here bro.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/contentScript.js"
      ],
      "all_frames": false,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "resources/icon16.png",
    "64": "resources/icon64.png",
    "128": "resources/icon128.png"
  },
  "browser_action": {
    "default_popup": "src/popup/popup.html"
  }
}